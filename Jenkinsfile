//--- Global Environment Variables.
pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo 'Building Sample Pipeline 01...'
				echo 'Building Sample Pipeline 02...'
				echo 'Building Sample Pipeline 03...'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing Sample Pipeline 01...'
				echo 'Testing Sample Pipeline 02...'
				echo 'Testing Sample Pipeline 03...'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying Sample Pipeline 01...'
				echo 'Deploying Sample Pipeline 02...'
				echo 'Deploying Sample Pipeline 03...'
			}
		}
	}
}
